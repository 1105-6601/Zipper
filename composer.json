{
    "name": "suemitsu/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "license": "Apache2",
    "require": {
        "php": ">=5.6.0",
        "ext-zip": "*",
        "illuminate/support": "^6.0",
        "illuminate/filesystem": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Zipper\\": "src/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
